<!DOCTYPE html>
<html>
<head>
  <title>Freshanalysis</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>	
</head>
<body>
		<%=render 'layouts/header' %>   <!--renders  the partial code stored in layouts/_header.html.erb-->

	<div class="container">  

		<!--creates flash message for devise, does not need = since html not generated-->
		<% flash.each do |name,msg| %> 
			<%= content_tag(:div, msg, class:"alert alert-info") %>
		<% end %>
		
		<%= yield %>

	</div>

</body>
</html>
